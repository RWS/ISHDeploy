{
  "build": {
    "content": [
      {
        "files": [ "toc.yml" ],
        "cwd": "obj/doc/commands",
        "dest": "Commands"
      },
      {
        "files": [ "**/*.md" ],
        "cwd": "obj/doc/commands",
        "dest": "Commands"
      },
      {
        "files": [ "toc.yml" ],
        "cwd": "Articles",
        "dest": "Articles"
      },
      {
        "files": [ "**/*.md" ],
        "cwd": "obj/doc/articles",
        "dest": "Articles"
      },
      {
        "files": [ "**/*.md" ],
        "cwd": "obj/doc/issues",
        "dest": "Issues"
      },
      {
        "files": [ "*.md" ],
        "cwd": "obj/doc",
        "dest": ""
      },
      {
        "files": [ "toc.yml" ]
      }
    ],
    "resource": [
      {
        "files": [ "**/images/**" ]
      },
      {
        "files": "sdl_logo_transparent.png"
      },
      {
        "files": "web.config"
      },
      {
        "files": [ "**/*.ps1","**/*.png" ],
        "cwd": "obj/doc/articles",
        "exclude": [ "**/_nopublish/**" ],
        "dest": "Articles"
      }
    ],
    "externalReference": [
    ],
    "globalMetadata": {
    },
    "template": [
      "default",
      "template"
    ],
    "dest": "_site"
  }
}