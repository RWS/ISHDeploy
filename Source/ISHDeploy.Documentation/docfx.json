{
  "build": {
    "content": [
      {
        "files": [ "toc.yml" ],
        "cwd": "obj/doc/module",
        "dest": "module"
      },
      {
        "files": [ "**/*.md" ],
        "cwd": "obj/doc/module/ISHDeploy",
        "dest": "module/ISHDeploy"
      },
      {
        "files": [ "toc.yml" ],
        "cwd": "obj/doc/using",
        "dest": "Using"
      },
      {
        "files": [ "toc.yml" ],
        "cwd": "Articles",
        "dest": "Articles"
      },
      {
        "files": [ "**/*.md" ],
        "cwd": "obj/doc/articles",
        "dest": "Articles"
      },
      {
        "files": [ "Index.md", "toc.yml" ]
      }
    ],
    "resource": [
      {
        "files": [ "**/images/**" ]
      },
      {
        "files": "sdl_logo_transparent.png"
      },
      {
        "files": "web.config"
      },
      {
        "files": [ "**/*.ps1" ],
        "cwd": "obj/doc/using",
        "dest": "Using"
      },
      {
        "files": [ "**/*.ps1" ],
        "cwd": "obj/doc/articles",
        "exclude": [ "**/_nopublish/**" ],
        "dest": "Articles"
      }
    ],
    "externalReference": [
    ],
    "globalMetadata": {
    },
    "template": [
      "default",
      "template"
    ],
    "dest": "_site"
  }
}